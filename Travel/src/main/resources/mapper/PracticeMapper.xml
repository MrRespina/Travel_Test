<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.travel.main.repository.PracticeMapper">
    <select id="selectById" resultType="com.travel.main.repository.Practice">
        SELECT * FROM prac.practice WHERE id = #{id}
    </select>
    <select id="selectAll" resultType="com.travel.main.repository.Practice">
        SELECT * FROM prac.practice
    </select>
    <insert id="joinMembership" parameterType="com.travel.main.repository.Practice">
    	INSERT INTO prac.practice (name,info) VALUES (#{name},#{info})
    </insert>
    <update id="updateMember" parameterType="com.travel.main.repository.Practice">
    	UPDATE prac.practice SET info = #{info} WHERE id = #{id}
    </update>
</mapper>